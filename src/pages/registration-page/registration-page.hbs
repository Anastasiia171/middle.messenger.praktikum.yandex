{{#> Dialog }}
<div class="registration-page-wrapper">
  <form class="registration-page">
    <div class="registration-header">   
      
      <h1 class="h1">Registration</h1>
    </div>
  
    <main class="registration-page-content">
      {{{ input_first_name }}}
      {{{ input_second_name }}}
      {{{ input_email }}}
      {{{ input_phone }}}
      {{{ input_login }}}
      {{{ input_password }}}
    </main>
    <div class="registration-page-footer">
      {{{ button }}} 
    </div>
  </form>
    <nav class="links">
    <a onclick="window.navigate('404-error')">404</a>
    <a onclick="window.navigate('settings')">settings-page</a>
    <a onclick="window.navigate('500-error')">500</a>
    <a onclick="window.navigate('login')">login-page</a>
    <a onclick="window.navigate('chat')">chat-page</a>
  </nav>
  </div>
{{/ Dialog }}

